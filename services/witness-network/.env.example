# Deployed address of the VortexVerifier contract
VORTEX_VERIFIER_ADDRESS="YOUR_VORTEX_VERIFIER_CONTRACT_ADDRESS"

# Interval in milliseconds to propose new data
PROPOSE_INTERVAL_MS=30000
